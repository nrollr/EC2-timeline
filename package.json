{
  "name": "ec2-timeline",
  "version": "1.0.0",
  "description": "A historical overview of AWS EC2 instance releases",
  "main": "./public/index.html",
  "scripts": {
    "build:css": "npx tailwindcss -i ./build/tailwind.css -o ./public/assets/css/style.css --watch",
    "build:minify": "cleancss -o ./public/assets/css/style.min.css ./public/assets/css/style.css --watch",
    "build:run": "npm-run-all --parallel build:css build:minify",
    "build:serve": "browser-sync start --server './public' --files './public'"
  },
  "dependencies": {
    "browser-sync": "^2.27.10",
    "clean-css": "^5.3.1",
    "clean-css-cli": "^5.6.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrollr/ec2-timeline.git"
  },
  "keywords": [],
  "author": "Nicolas Rollier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrollr/ec2-timeline/issues"
  },
  "homepage": "https://instancetyp.es"
}